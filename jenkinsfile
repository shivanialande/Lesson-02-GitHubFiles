pipeline{
    agent any
     stages{
         stage('GITCheckout'){
             steps{
                git credentialsId: 'github', url: 'https://github.com/shivanialande/Lesson-02-GitHubFiles'
             }
         }    
         stage("Maven Build"){
             steps{
                sh "mvn clean package"
             } 
         }
         stage("Building of Docker image"){
             steps{
                 sh 'docker build -t shivanialande/nginx:1.0 -f dockerfile'
             }
         }
    }
}
